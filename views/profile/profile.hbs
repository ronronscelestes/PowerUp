<h2>wantToPlay</h2>
{{#each user.wantToPlay}}
<div>{{this.name}}</div>
{{!-- <button class="moveToCurrentPlay" >CurrentPlay</button>
<button class="moveToAlreadyPlayed" >Alreadyplayed</button> --}}
{{/each}}

<h2>CurrentPlay</h2>
<div>
{{#each user.currentPlay}}
<div>{{this.name}}</div>
{{!-- <button class="moveToWantToPlay" >WantToPlay</button>
<button class="moveToAlreadyPlayed" >Alreadyplayed</button> --}}
{{/each}}
</div>

<h2>alreadyPlayed</h2>
